{
  "name": "PatientApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "build-android": "cd ./android && ./gradlew app:assembleRelease",
    "clean-android": "cd ./android && ./gradlew clean && cd .. && npx react-native bundle --platform android --dev false  --entry-file index.js --reset-cache --bundle-output android/app/src/main/assets/index.android.bundle",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package && npx typesync",
    "asset": "npx react-native-asset",
    "pod": "pod install --project-directory=ios",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='main.jsbundle' --dev=false --platform='ios'",
    "push-ios": "appcenter codepush release-react -a tranvantai.tvt141/Patient_app -d Staging",
    "push-android": "appcenter codepush release-react -a tranvantai.tvt141/Patient_app_android -d Staging",
    "port": "react-native start --port 9988"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-community/push-notification-ios": "^1.10.1",
    "@react-native-firebase/app": "^16.4.3",
    "@react-native-firebase/messaging": "^16.4.3",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/stack": "^6.3.11",
    "@testing-library/react-native": "^11.5.2",
    "appcenter": "^4.4.5",
    "axios": "^1.1.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "i18n-js": "^3.8.2",
    "jwt-decode": "^3.1.2",
    "libphonenumber-js": "^1.10.19",
    "lottie-react-native": "^5.1.4",
    "moment": "^2.29.4",
    "patch-package": "^6.5.1",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.5.0",
    "react-native-confirmation-code-field": "^7.3.1",
    "react-native-device-info": "^10.3.0",
    "react-native-document-picker": "^8.1.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-gifted-charts": "^1.2.42",
    "react-native-gifted-chat": "^1.1.1",
    "react-native-health": "^1.14.0",
    "react-native-image-picker": "^5.1.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-incoming-call": "^2.0.3",
    "react-native-keep-awake": "^4.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mime-types": "^2.3.0",
    "react-native-modalize": "^2.1.1",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-pager-view": "^6.1.4",
    "react-native-permissions": "^3.6.1",
    "react-native-pinchable": "^0.2.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0",
    "react-native-sensitive-info": "^5.5.8",
    "react-native-shadow-2": "^7.0.6",
    "react-native-sound": "^0.11.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.7.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tab-view": "^3.5.1",
    "react-native-twilio-video-webrtc": "https://github.com/blackuy/react-native-twilio-video-webrtc",
    "react-native-video": "^5.2.1",
    "react-redux": "^7.2.6",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "rn-fetch-blob": "github:joltup/rn-fetch-blob",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/babel__core": "^7.20.0",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.21.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^26.0.23",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-native-keep-awake": "^2.0.3",
    "@types/react-native-push-notification": "^8.1.1",
    "@types/react-native-video": "^5.0.14",
    "@types/react-redux": "^7.1.25",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rn-fetch-blob": "^1.2.3",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "test-config",
      "interfaces",
      "jestGlobalMocks.ts",
      ".module.ts",
      ".mock.ts",
      "<rootDir>/src/hooks",
      "<rootDir>/src/assets",
      "<rootDir>/src/components",
      "<rootDir>/src/globalState",
      "<rootDir>/src/helper",
      "<rootDir>/src/languages",
      "<rootDir>/src/models",
      "<rootDir>/src/services",
      "<rootDir>/src/navigation"
    ]
  },
  "comment": {
    "splash_screen": "react-native-splash-screen reference in Libraries folder!",
    "android:icon": "https://romannurik.github.io/AndroidAssetStudio/icons-launcher.html#foreground.type=image&foreground.space.trim=1&foreground.space.pad=0.25&foreColor=rgba(96%2C%20125%2C%20139%2C%200)&backColor=rgb(7%2C%20116%2C%20228)&crop=1&backgroundShape=circle&effects=none&name=ic_launcher",
    "notification": "https://stackoverflow.com/questions/27324020/no-valid-aps-environment-entitlement-string-found-for-application-on-app-store",
    "free_icons": "https://thenounproject.com/icon/",
    "fix_async_call_chrome": "https://stackoverflow.com/questions/61067004/invariant-violation-calling-synchronous-methods-on-native-modules-is-not-suppor"
  }
}
